#include<iostream>
#include<cmath>
using namespace std;

int main()
{
	
	double x=2.3;
	cout<<"Sine value of x=2.3: "<< sin(x)<<"\n";
	cout<<"Cosine value of x=2.3"<<cos(x)<<"\n";
	cout<<"Tangent value of x=2.3"<<tan(x)<<"\n";
	
	double y=0.25;
	cout <<"Square root value pf y=0.25 : "<<sqrt(y)<<"\n";
	
	int z= -10;
	cout << "Absolute value of z=-10" << abs(z)<<"\n";
	cout<< "Power value of x^y=(2.3^0.25) : " << pow(x, y)<<"\n";
	
	x=3.0;
	y=4.0;
	cout <<"Hypotenuse having other two sides as x=3.0 and "<<"y=4.0: " << hypot(x,y)<<"\n";
	
	x=4.56;
	cout<<"Floor value of x=4.56 is : "<<floor(x)<<"\n";
	
	x=-4.57;
	cout<<"Absolute value of x=-4.57 is : "<<f abs(x)<<"\n";
	
	x=1.0;
	cout<<"Arc Cosine value of x=1.0 : "<<acos(x)<<"\n";
	cout<<"Arc Sine value of x=1.0 : "<<asin(x)<<"\n";
	cout<<"Arc Tangent value of x=1.0 : "<<atan(x)<<"\n";
	
	y=12.3;
	cout <<"Ceiling value of y=12.3 : "<<ceil(y)<<"\n";
	
	x=57.3; //in degrees
	cout<<"Hyperbolic Cosine of x=57.3 : "<<cosh(x)<<"\n";
	cout<<"Hyperbolic tangent of x=57.3 : "<<tanh(x)<<"\n";
	
	y=100.0;
	//natural base with 'e'
	cout<<"Log value of y=100.0 is: "<<log(y)<<"\n";
	
	return 0;
}
